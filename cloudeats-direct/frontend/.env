# Frontend uses relative API endpoints so nginx can proxy inside Docker
REACT_APP_INVENTORY_SERVICE_URL=/api/inventory
REACT_APP_ORDER_SERVICE_URL=/api/orders
REACT_APP_FLEET_SERVICE_URL=/api/fleet
REACT_APP_NOTIFICATION_SERVICE_URL=/api/notification
REACT_APP_PAYMENT_SERVICE_URL=/api/payment

# Frontend port used by docker-compose mapping; keep as 3000 for host
FRONTEND_PORT=3000
